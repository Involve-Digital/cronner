{
	"name": "involve-digital/cronner",
	"type": "nette-addon",
	"description": "Simple tool which helps with maintenance of cron tasks.",
	"keywords": ["nette", "cron", "manager", "task"],
	"license": "MIT",
	"authors": [
		{
			"name": "Josef Petrák",
			"email": "me@jspetrak.name",
			"role": "Developer"
		},
		{
			"name": "Martin Štekl",
			"email": "martin.stekl@gmail.com",
			"role": "Developer"
		}
	],
	"support": {
		"email": "development@bileto.com",
		"issues": "https://github.com/bileto/cronner/issues",
		"source": "https://github.com/bileto/cronner"
	},
	"require": {
		"php": ">=8.0",
		"tracy/tracy": "^2.8",
		"nette/di": "^3.0",
		"nette/bootstrap": "^3.0",
		"nette/utils": "^3.0",
		"involve-digital/reflection": "dev-php-8",
		"nette/safe-stream": "^2.3",
		"bileto/critical-section": "^2.1"
	},
	"require-dev": {
		"nette/tester": "^1.7",
		"jakub-onderka/php-parallel-lint": "^0.9",
		"mockery/mockery": "^0.9",
		"phpstan/phpstan": "^1.10"
	},
	"repositories": [
		{
			"type": "github",
			"url": "git@github.com:involve-digital/reflection.git"
		}
	],
	"suggest": {
	},
	"replace": {
		"stekycz/cronner": "v2.2.0"
	},
	"autoload": {
		"psr-4": {
			"stekycz\\": "src"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/CronnerTests/objects"]
	}
}
