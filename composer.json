{
	"name": "bileto/cronner",
	"type": "nette-addon",
	"description": "Simple tool which helps with maintenance of cron tasks.",
	"keywords": ["nette", "cron", "manager", "task"],
	"license": "MIT",
	"authors": [
		{
			"name": "Josef Petrák",
			"email": "me@jspetrak.name",
			"role": "Developer"
		},
		{
			"name": "Martin Štekl",
			"email": "martin.stekl@gmail.com",
			"role": "Developer"
		}
	],
	"support": {
		"email": "development@bileto.com",
		"issues": "https://github.com/bileto/cronner/issues",
		"source": "https://github.com/bileto/cronner"
	},
	"require": {
		"php": ">=7.1.0",
		"ext-calendar": "*",
		"tracy/tracy": "^2.4",
		"nette/di": "^2.4",
		"nette/bootstrap": "^2.4",
		"nette/utils": "^2.4",
		"nette/reflection": "^2.4",
		"nette/safe-stream": "^2.3",
		"bileto/critical-section": "^2.1"
	},
	"require-dev": {
		"nette/tester": "^2.3",
		"jakub-onderka/php-parallel-lint": "^0.9",
		"mockery/mockery": "^1.3",
		"phpstan/phpstan": "^0.12"
	},
	"suggest": {
		"kdyby/events": "Events for Nette Framework",
		"kdyby/console": "Symfony Console integration for Kdyby components"
	},
	"autoload": {
		"psr-4": {
			"Bileto\\": "src"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/CronnerTests/Objects"]
	}
}
